<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <servlet>
  	<servlet-name>mine</servlet-name>
  	<servlet-class>com.online.test.ServletConfigDemo</servlet-class>
  	
  	<init-param>
  		<param-name>wish</param-name>
  		<param-value>welcome</param-value>
  	</init-param>
  	
  	<init-param>
  		<param-name>name</param-name>
  		<param-value>Bye</param-value>
  	</init-param>

  </servlet>
  <servlet-mapping>
  	<servlet-name>mine</servlet-name>
  	<url-pattern>/config123</url-pattern>
  </servlet-mapping>
  
  <context-param>
  		<param-name>driver</param-name>
  		<param-value>jdbc:mysql</param-value>
  </context-param>
  <context-param>
  		<param-name>username</param-name>
  		<param-value>system</param-value>
  </context-param>
  
 	<servlet>
 		<servlet-name>Login</servlet-name>
 		<servlet-class>com.online.LoginServlet</servlet-class>
 	</servlet>
 	<servlet-mapping>
 		<servlet-name>Login</servlet-name>
 		<url-pattern>/login</url-pattern>
 	</servlet-mapping>
 	
 	<context-param>
 		<param-name>driver</param-name>
 		<param-value>com.mysql.jdbc.Driver</param-value>
 	</context-param>
 	<context-param>
 		<param-name>url</param-name>
 		<param-value>jdbc:mysql://localhost:3306/first?serverTimezone=EST</param-value>
 	</context-param>
 	<context-param>
 		<param-name>username</param-name>
 		<param-value>root</param-value>
 	</context-param>
 	<context-param>
 		<param-name>password</param-name>
 		<param-value>cloudera</param-value>
 	</context-param>

</web-app>